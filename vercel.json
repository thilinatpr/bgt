{
  "version": 2,
  "builds": [
    {
      "src": "api/tasks.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/tasks(.*)",
      "dest": "/api/tasks"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}